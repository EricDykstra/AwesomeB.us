<div class="route-info">
  <ul id="input">
    <form class="user-input">
      <li><input type="text" id="start_loc" value="Current Location"></li>
      <li id="end"><input type="text" id="end_loc" placeholder="Destination" autofocus="autofocus"></li>
    </form>
  </ul>
  <ul id="fetch" class="hidden">
    <li><input type="text" readonly="readonly" id="fetch" value="Route..."></li>
    <li><input type="text" readonly="readonly" id="fetch" value="Leave At..."></li>
    <li><input type="text" readonly="readonly" id="fetch" value="Arrive At..."></li>
  </ul>
  <div class="current-route hidden">
    <div class="this-route" id="route-name"><p>L</p></div>
    <div class="this-route" id="leave-at"><p>8:17</p></div>
    <div class="this-route" id="arrive-at"><p>9:00</p></div>
  </div>
</div>

<div class="center-div">
  <a class="go"><%= image_tag "go.png", class: "go" %></a>
  <%= image_tag "ajax-loader1.gif", class: "spin hidden" %>
</div>

<div class="bottom-div">
  <!-- <div class="timer"/> -->
    <%= image_tag "circlepurple.png", class: "circle hidden" %>
    <%= image_tag "circlegreen.png", class: "circle hidden" %>
  <!-- <div class="homeclearsomething"/> -->
    <a href="/"><%= image_tag "home.png", class: "home-nav" %></a>
</div>
